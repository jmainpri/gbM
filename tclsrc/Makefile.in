top_srcdir=	@top_srcdir@
include		../Init.make

INSTALL=	@INSTALL@
INSTALL_DATA=	@INSTALL_DATA@

TCL_FILES=	gbModeles.tcl  gbModelesTest.tcl \
	gb_p3d.tcl  gb.tcl  gbTest.tcl

all:	
	@echo "Nothing to be done here"

install:
	@for f in $(TCL_FILES); do \
		echo "installing $$f in $(DESTDIR)$(GBM_TCL_DIR)"; \
		$(INSTALL_DATA) @srcdir@/$$f $(DESTDIR)$(GBM_TCL_DIR) ; \
	done
	tclsh @srcdir@/mkIndex

depend:
	@echo "No dependencies here"

clean:
	@echo "Nothing to clean here"

distclean:
	rm -f Makefile
